{
    "name": "keithbrink/amazon-mws-laravel",
    "description": "A Laravel library for Amazon's MWS web services, supporting Laravel 5-7+",
    "license": "Apache-2.0",
    "keywords": ["API", "Amazon", "PHP", "MWS", "Laravel"],
    "authors": [
        {
            "name": "Keith Brink",
            "email": "keith.brink@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "symfony/http-kernel": "^5.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "illuminate/http": "^7.0",
        "illuminate/routing": "^7.0",
        "illuminate/view": "^7.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^5.7",
        "vlucas/phpdotenv": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "KeithBrink\\AmazonMws\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KeithBrink\\AmazonMws\\MwsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
